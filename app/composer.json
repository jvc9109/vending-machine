{
  "name": "jvc9109/vending_machine",
  "description": "PoC of a vending Machine",
  "license": "proprietary",
  "require": {
    "php": "^8.0",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-intl": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "lambdish/phunctional": "^2.1",
    "doctrine/dbal": "^2.13",
    "doctrine/orm": "^2.9",
    "doctrine/cache": "^2.0",

    "ramsey/uuid": "^4.0",
    "symfony/cache": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/yaml": "5.2.*",
    "symfony/messenger": "5.2.*",
    "symfony/monolog-bundle": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "VendingMachine\\Apps\\Vending\\Backend\\": "apps/vending/backend/src",
      "VendingMachine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VendingMachine\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/debug-bundle": "*",
    "symfony/monolog-bundle": "^3.0",
    "phpunit/phpunit": "^9.5",
    "cyberclick-os/faker": "*",
    "mockery/mockery": "^1.4"
  }
